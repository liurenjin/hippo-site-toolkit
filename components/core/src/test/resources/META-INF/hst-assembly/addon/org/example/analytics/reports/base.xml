<?xml version="1.0" encoding="UTF-8"?>
<beans 
  xmlns="http://www.springframework.org/schema/beans"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">

  <bean id="myGreeting" class="java.lang.String">
    <constructor-arg value="[Analytics Reports] ${with.addon.modules.greeting}" />
  </bean>
  
  <bean id="analyticsReportsGreeting" class="java.lang.String">
    <constructor-arg value="Hello from analytics reports" />
  </bean>
  
  <bean id="greetingList" class="org.springframework.beans.factory.config.ListFactoryBean">
    <property name="sourceList">
      <list>
        <ref bean="containerGreeting" />
        <ref bean="analyticsGreeting" />
        <ref bean="analyticsReportsGreeting" />
      </list>
    </property>
  </bean>

</beans>