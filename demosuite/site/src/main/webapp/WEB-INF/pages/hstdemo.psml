<?xml version="1.0" encoding="UTF-8"?>
<page id="/hstdemo.psml" hidden="false">

  <title>HST-2 Demo</title>
  <short-title>HST-2 Demo</short-title>

  <fragment id="hstdemo" type="layout" name="jetspeed-layouts::VelocityTwoColumns2575">
    
    <fragment id="hstdemo-weather" type="portlet" name="demo::WeatherPortlet">
        <property layout="TwoColumns" name="row" value="0"/>
        <property layout="TwoColumns" name="column" value="0"/>
    </fragment>
    
    <fragment id="hstdemo-cmstodolist" type="portlet" name="site::CMSTodoListPortlet" >
      <property layout="TwoColumns" name="row" value="1" />
      <property layout="TwoColumns" name="column" value="0" />
    </fragment>
    
    <fragment id="hstdemo-bookmark" type="portlet" name="demo::BookmarkPortlet">
      <property layout="TwoColumns" name="row" value="2" />
      <property layout="TwoColumns" name="column" value="0" />
    </fragment>
    
    <fragment id="hstdemo-cmssearch" type="portlet" name="site::CMSSearchPortlet">
      <property layout="TwoColumns" name="row" value="0"/>
      <property layout="TwoColumns" name="column" value="1"/>
    </fragment>
    
    <fragment id="hstdemo-newsdemo" type="portlet" name="site::NewsDemoPortlet">
      <property layout="TwoColumns" name="row" value="1"/>
      <property layout="TwoColumns" name="column" value="1"/>
    </fragment>
    
  </fragment>

  <security-constraints>
    <security-constraints-ref>public-view</security-constraints-ref>
  </security-constraints>

</page>
